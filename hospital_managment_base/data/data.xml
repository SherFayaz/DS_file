<odoo>
    <record id="doctor_sequence_id" model="ir.sequence">
        <field name="name">Doctor sequence</field>
        <field name="code">galaxy.doctor</field>
        <field name="prefix">DR/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

      <record id="patient_sequence_id" model="ir.sequence">
        <field name="name">patient sequence</field>
        <field name="code">galaxy.patient</field>
        <field name="prefix">PA/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>